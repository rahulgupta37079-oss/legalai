{
  "name": "webapp",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && cp public/static/app.js dist/static/app.js",
    "dev:sandbox": "wrangler pages dev dist --d1=legal-ai-production --local --ip 0.0.0.0 --port 3000",
    "preview": "wrangler pages dev",
    "deploy": "npm run build && wrangler pages deploy dist",
    "deploy:prod": "npm run build && wrangler pages deploy dist --project-name legal-ai-platform",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:migrate:local": "wrangler d1 migrations apply legal-ai-production --local",
    "db:migrate:prod": "wrangler d1 migrations apply legal-ai-production",
    "clean-port": "fuser -k 3000/tcp 2>/dev/null || true"
  },
  "dependencies": {
    "@cloudflare/workers-types": "^4.20260131.0",
    "@hono/vite-cloudflare-pages": "^0.4.3",
    "bcryptjs": "^3.0.3",
    "hono": "^4.11.7",
    "jsonwebtoken": "^9.0.3"
  },
  "devDependencies": {
    "@hono/vite-build": "^1.2.0",
    "@hono/vite-dev-server": "^0.18.2",
    "vite": "^6.3.5",
    "wrangler": "^4.4.0"
  },
  "version": "1.0.0",
  "description": "```txt npm install npm run dev ```",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
